<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <staticContent>
            <mimeMap fileExtension=".air" mimeType="application/vnd.adobe.air-application-installer-package+zip" />
        </staticContent>
        <rewrite>
            <rewriteMaps>
                <rewriteMap name="http://mysite.local/" />
            </rewriteMaps>
            <rules>
                <rule name="HTTP" stopProcessing="true">
                    <match url=".*" />
                    <conditions>
                        <add input="{HTTP_HOST}" pattern="off" />
                    </conditions>
                    <action type="Redirect" url="http://mysite.local/" redirectType="Found" />
                </rule>
                <!-- <rule name="rule login.cfm" stopProcessing="true">
                    <match url="^login$" />
                    <action type="Rewrite" url="/index.cfm?action=login" />
                </rule>
                <rule name="rule signUp.cfm" stopProcessing="true">
                    <match url="^signup$" />
                    <action type="Rewrite" url="/index.cfm?action=signup" />
                </rule> -->
                <rule name="rule login.cfm">
                    <match url="^login$" />
                    <action type="Redirect" url="/views/listingPage.cfm" />
                </rule>
                <rule name="rule signUp.cfm">
                    <match url="^signup$" />
                    <action type="Redirect" url="/views/signUp.cfm" />
                </rule>
                <rule name="rule listingPage.cfm">
                    <match url="^display$" />
                    <action type="Redirect" url="/views/listingPage.cfm" />
                </rule>
            </rules>
            <outboundRules>
                <rule name="rule iisLogin">
                    <match filterByTags="A" pattern="^/views/login\.cfm$" />
                    <action type="Rewrite" value="login" />
                </rule>
                <rule name="rule iisSignUp">
                    <match filterByTags="A" pattern="^/views/signUp\.cfm$" />
                    <action type="Rewrite" value="signup" />
                </rule>
                <rule name="rule iisDisplay">
                    <match filterByTags="A" pattern="^/views/listingPage\.cfm$" />
                    <action type="Rewrite" value="display" />
                </rule>

            </outboundRules>
        </rewrite>
    </system.webServer>
</configuration>




